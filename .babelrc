{
    "env": {
        "production": {
            "plugins": [
                [
                    "styled-components",
                    {
                        "displayName": false,
                    }
                ],
            ]
        }
    },
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "styled-components",
            {
                "ssr": true,
                "preprocess": false
            }
        ],
        "styled-components-postcss"
    ]
}
